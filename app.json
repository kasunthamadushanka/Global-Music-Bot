{
    "name": "GLOBAL_MUSIC_BOT",
    "logo": "https://telegra.ph/file/07c49a0be5f7a9c7752a1.jpg",
    "description": "Open-Source bot to play songs in your Telegram's Group Voice Chat. Powered by PyTgCalls.",
    "keywords": ["music", "voicechat", "telegram"],
    "repository": "https://github.com/kasunthamadushanka/Global-Music-Bot",
    "stack": "container",
    "env": {
        "SESSION_NAME": {
            "description": "Session string (pyrogram)",
            "required": true
        },
        "BOT_TOKEN": {
            "description": "From @BotFather",
            "required": true
        },
        "BOT_USERNAME": {
            "description": "Your Bot Username without @.",
            "required": true,
            "value": ""
       },
        "API_ID": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "API_HASH": {
            "description": "From my.telegram.org/apps",
            "required": true
        },
        "SUDO_USERS": {
            "description": "Full-access users, counted as admin everywhere, seperated by space.",
            "required": true
        },
        "DURATION_LIMIT": {
            "description": "In minutes!",
            "required": true,
            "value": "15"
        }
    },
    "buildpacks": [
       {
      "url": "heroku/python"
       }
     ]
  }
